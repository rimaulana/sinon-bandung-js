{
    "name": "sinon-bandung",
    "version": "1.0.0",
    "description": "Collection of codes showing how to use Sinon.js in Bandung.js meetup",
    "main": "index.js",
    "scripts": {
        "test": "node_modules/mocha/bin/mocha --reporter=spec tests/*.js",
        "cover": "node_modules/nyc/bin/nyc.js --reporter=text-summary node_modules/mocha/bin/mocha --reporter=nyan tests/*.js"
    },
    "keywords": [
        "sinon",
        "bandung.js",
        "testing"
    ],
    "nyc": {
        "exclude": [
            "tests/*.js"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rimaulana/sinon-bandung-js.git"
    },
    "author": "Rio Maulana <rimaulana@outlook.com> (https://github.com/rimaulana)",
    "license": "MIT",
    "dependencies": {
        "aws-sdk": "^2.193.0"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "mocha": "^5.0.0",
        "nyc": "^11.7.1",
        "sinon": "^4.3.0"
    }
}